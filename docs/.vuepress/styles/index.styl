.theme-default-content:not(.custom) > h2 {
  margin-top: 1.2rem;
  padding-top: 0;
}

.theme-default-content pre, .theme-default-content pre[class*="language-"] {
  margin: 0;
}

.theme-default-content .code1 pre {
  border-radius: 0;
}

.theme-default-content .code2 pre {
  border-radius: 0 0 6px 6px;
}